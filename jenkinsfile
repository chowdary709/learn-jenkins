pipeline {
    agent {
        node {
            label 'workstation-prasad' // Ensure this label matches your CentOS node
        }
    }

    stages {
        stage('Build') {
            steps {
                echo 'building..'
            }
        }
        stage('Test') {
            steps {
                echo 'testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploying..'
            }
        }
        stage('dev') {
            steps {
                echo 'dev'
            }
        }
    }
    post {
        always {
            echo 'I will always say Hello again!'
        }
        failure {
            echo 'I am running because your script failed'
        }
        success {
            echo 'I am running because your script succeeded'
        }
    }
}
