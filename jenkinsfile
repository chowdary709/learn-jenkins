pipeline {
    agent {
        node {
            label 'workstation-prasad'
        }
    }

    stages {
        stage('one') {
            steps {
                echo 'Stage one'
                sh 'echo "Hello from stage one"'
            }
        }
        stage('two') {
            steps {
                echo 'Stage two'
                sh '''
                ls -a
                pwd
                cal
                '''
            }
        }
    }
}
