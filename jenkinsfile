pipeline {
    agent {
        node {
            label 'workstation-prasad' // Ensure this label matches your CentOS node
        }
    }

    stages {
        stage('time') {
            steps {
                script {
                    def currentTime = sh(script: 'date', returnStdout: true).trim()
                    echo "Current time is: ${currentTime}"
                }
            }
        stage('Build') {
            steps {
                echo 'building..'
            }
        }
        stage('Test') {
            steps {
                echo 'testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploying..'
            }
        }
    }
}

